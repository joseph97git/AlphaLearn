(function ($) {

    // openstreetmap base layer
    // var rasterLayer = new ol.layer.Tile({
    //     source: new ol.source.OSM()
    // });

    var rasterLayer = new ol.layer.Tile({
        source: new ol.source.BingMaps({
            key: 'AqnL9AaU3nzGzEbJpDyb0GKPKt3gedvqOJemuIpg2R1IhvHwJnwSs24FbtY4izzD',
            imagerySet: 'Aerial'
            // use maxZoom 19 to see stretched tiles instead of the BingMaps
            // "no photos at this zoom level" tiles
            // maxZoom: 19
        })
    });

    var minZoomConfig = 2.7
    if((jQuery(window).width() < 768) || (jQuery(window).width() > 768 && jQuery(window).height() < 667)) {
        minZoomConfig = 1;
    }
    if(jQuery(window).width() >= 768 && jQuery(window).width() < 1024 ) {
        minZoomConfig = 1.5;
    } 
    if(jQuery(window).width() >= 1024 && jQuery(window).width() < 1600 ) {
        minZoomConfig = 2.3;
    }
    if( (jQuery(window).width() > 1200 && jQuery(window).height() < 667) || (jQuery(window).width() == 1024 && jQuery(window).height() == 768) || (jQuery(window).width() == 1024 && jQuery(window).height() == 1366)) {
        minZoomConfig = 2.1;
    }

    var view = new ol.View({
        center: ol.proj.fromLonLat([28.9744, 41.0128]),
        projection: 'EPSG:3857',
        minZoom: minZoomConfig, 
        maxZoom: 5, 
        zoom:3
    });

    var defaultStyle = new ol.style.Style({
        stroke: new ol.style.Stroke({
            color: [255, 255, 255, 1],
            width: 0.6
        })
    });

    var selectStyle = new ol.style.Style({
        fill: new ol.style.Fill({
            color: 'rgba(255, 255, 255, 0.6)'
        })
        // stroke: new ol.style.Stroke({
        //     color: 'rgba(255, 255, 255, 0.6)',
        //     width: 1
        // })
    });


    var vectorSource = new ol.source.Vector({
        format: new ol.format.GeoJSON({}),
        url: drupalSettings.path.baseUrl + 'themes/custom/wb_cckp/resources/data/geojson-countries.json'
        //url: 'https://d3j0tr2xa7p0hd.cloudfront.net/geojson-countries-p2.min.json'
    });

    var vectorLayer = new ol.layer.Vector({
        source: vectorSource,
        style: defaultStyle,
        name: 'countries',
        visible: true
    });


    var map = new ol.Map({
        layers: [rasterLayer, vectorLayer],
        target: 'country_map_container',
        interactions: ol.interaction.defaults({mouseWheelZoom: false}),
        view: view
    });


    vectorSource.once('change', function (e) {
        if (vectorSource.getState() === 'ready') {
            var extent = vectorSource.getExtent();
            view.fit(extent, map.getSize());
        }
    });


    var selectInteraction = new ol.interaction.Select({
        condition: ol.events.condition.click,
        layer: vectorLayer,
        style: selectStyle
    });
    map.addInteraction(selectInteraction);

    var hoverInteraction = new ol.interaction.Select({
        condition: ol.events.condition.pointerMove,
        layer: vectorLayer,
        style: selectStyle
    });
    map.addInteraction(hoverInteraction);

    var tooltip = document.getElementById('country-tooltip');
    var overlay = new ol.Overlay({
        element: tooltip,
        offset: [10, 0],
        positioning: 'bottom-left'
    });
    map.addOverlay(overlay);

    function displayTooltip(evt) {
        var pixel = evt.pixel;
        var feature = map.forEachFeatureAtPixel(pixel, function (feature) {
            return feature;
        });
        //tooltip.style.display = feature ? '' : 'none';
        if (feature) {
            overlay.setPosition(evt.coordinate);
	    if(feature.R.slug != 'western-sahara' && feature.R.slug != 'gaza-strip' && feature.R.slug != 'wallis-and-futuna' && feature.R.slug != 'anguilla'    && feature.R.slug != 'british-indian-ocean-territory' && feature.R.slug != 'christmas-island' && feature.R.slug != 'cocos-keeling-islands' && feature.R.slug != 'cook-islands' && feature.R.slug != 'falkland-islands-islas-malvinas' && feature.R.slug != 'french-guiana' && feature.R.slug != 'guadeloupe' && feature.R.slug != 'martinique' && feature.R.slug != 'mayotte' && feature.R.slug != 'montserrat' && feature.R.slug != 'netherlands-antilles' && feature.R.slug != 'niue' && feature.R.slug != 'norfolk-island' && feature.R.slug != 'pitcairn-islands' && feature.R.slug != 'st-helena' && feature.R.slug != 'svalbard' && feature.R.slug != 'tokelau'  ) {
				tooltip.style.display = "block";
				tooltip.innerHTML = feature.R.ADM0_NAME;
			}
			else {
				tooltip.style.display = "none";
			}
        }
    }

    map.on('pointermove', displayTooltip);

    map.on('singleclick', function (e) {
        var features = map.getFeaturesAtPixel(e.pixel);
        country_slug = features[0].R.slug;
	if(features[0].R.slug != 'western-sahara' && features[0].R.slug != 'gaza-strip' && features[0].R.slug != 'wallis-and-futuna' && features[0].R.slug != 'anguilla'    && features[0].R.slug != 'british-indian-ocean-territory' && features[0].R.slug != 'christmas-island' && features[0].R.slug != 'cocos-keeling-islands' && features[0].R.slug != 'cook-islands' && features[0].R.slug != 'falkland-islands-islas-malvinas' && features[0].R.slug != 'french-guiana' && features[0].R.slug != 'guadeloupe' && features[0].R.slug != 'martinique' && features[0].R.slug != 'mayotte' && features[0].R.slug != 'montserrat' && features[0].R.slug != 'netherlands-antilles' && features[0].R.slug != 'niue' && features[0].R.slug != 'norfolk-island' && features[0].R.slug != 'pitcairn-islands' && features[0].R.slug != 'st-helena' && features[0].R.slug != 'svalbard' && features[0].R.slug != 'tokelau' ) {
			tooltip.style.display = "block";
			window.location.href = drupalSettings.path.baseUrl + "country/" + country_slug;
		}
    });


}(jQuery));

