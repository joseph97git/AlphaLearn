    nav_tab: function(fn_options) {

      var plugin_instance = this;
      var plugin_item = this.item;
      var plugin_settings = plugin_instance.options;
      var plugin_elements = plugin_settings.elements;
      
      // options

      var defaults = {
        tab: 'country'
      };

      var settings = $.extend(true, defaults, fn_options);
      
      var current_tab = $('#nav-overlay-content').attr('data-tab');
      
      if (settings.tab !== current_tab) {
        
        // console.log(settings.tab);
        
        // tab list item class
        
        $('#nav-overlay-tabs li').removeClass('selected');
        $('#nav-overlay-tabs li[data-tab="' + settings.tab + '"]').addClass('selected');
        
        // content data attribute
        
        $('#nav-overlay-content').attr('data-tab', settings.tab);
        
        // current tab class
        
        $('.nav-overlay-tab').removeClass('current-tab');
        $('#nav-tab-' + settings.tab).addClass('current-tab');
        
      }
      
    },
    
    show_nav: function(fn_options) {

      var plugin_instance = this;
      var plugin_item = this.item;
      var plugin_settings = plugin_instance.options;
      var plugin_elements = plugin_settings.elements;
      
      // options

      var defaults = {
        tab: 'country'
      };

      var settings = $.extend(true, defaults, fn_options);
      
      $('body').addClass('nav-overlay-open');
      
      plugin_instance.nav_tab({
        tab: settings.tab
      });
      
    },
    
    hide_nav: function(fn_options) {

      var plugin_instance = this;
      var plugin_item = this.item;
      var plugin_settings = plugin_instance.options;
      var plugin_elements = plugin_settings.elements;
      
      $('body').removeClass('nav-overlay-open');
      
    }
    
  }

  // jQuery plugin interface

  $.fn.cckp = function (opt) {
    var args = Array.prototype.slice.call(arguments, 1);

    return this.each(function () {

      var item = $(this);
      var instance = item.data('cckp');

      if (!instance) {

        // create plugin instance if not created
        item.data('cckp', new cckp(this, opt));

      } else {

        // otherwise check arguments for method call
        if (typeof opt === 'string') {
          instance[opt].apply(instance, args);
        }

      }
    });
  }

}(jQuery));

