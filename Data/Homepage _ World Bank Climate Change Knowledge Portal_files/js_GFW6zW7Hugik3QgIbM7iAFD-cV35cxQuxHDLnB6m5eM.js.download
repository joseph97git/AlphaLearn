// window spec
// v1.0

(function ($) {
  
  $.fn.window_spec = function(options) {
    var settings = $.extend({
      option: 'width',
      fraction: 1
    }, options);
    
    var spec;
    
    if (settings.option == 'width') {
      spec = $(window).width() / settings.fraction;
    } else if (settings.option == 'height') {
      spec = $(window).height() / settings.fraction;
    }
    
    return spec;
  }
  
}(jQuery));
