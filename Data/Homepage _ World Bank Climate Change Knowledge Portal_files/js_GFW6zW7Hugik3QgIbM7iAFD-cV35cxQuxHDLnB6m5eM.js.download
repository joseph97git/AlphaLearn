      // NAV OVERLAY TAB CLICK
      
      $('#nav-overlay-tabs li').click(function(e) {
        e.preventDefault();
        
        var new_tab = $(this).attr('data-tab');
        
        if(new_tab == "watershed") {
          $('#nav-overlay-options .option-block.or').hide();
          $('#nav-overlay-options div#option-list').hide();
          jQuery('#nav-overlay-view-map').trigger('click');
        } else {
          $('#nav-overlay-options .option-block.or').show();
          $('#nav-overlay-options div#option-list').show();
        }
        
        plugin_instance.nav_tab({
          tab: new_tab
        });
        
      });
      
      // NAV OVERLAY VIEW TOGGLE
      
      $('#nav-overlay .view-toggle').click(function() {
      
        var new_view = $(this).attr('data-view');
        
        $(this).parents('#nav-overlay-options').find('.view-toggle.selected').removeClass('selected');
        $(this).addClass('selected');
        
        $('#nav-overlay-content').attr('data-view', new_view);
        
      });
      
      // NAV OVERLAY CLOSE
      
      $('#nav-overlay-close').click(function(e) {
      
        e.preventDefault();
        jQuery("html").css("overflow", "auto");
        
        $('body').removeClass('nav-overlay-open');
        
      });
      
